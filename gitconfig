[alias]
    st = status
    s = status
    ff = flow feature
    ffs = flow feature start
    fff = flow feature finish
    ffp = flow feature publish
    ffc = flow feature checkout
    graph = log --branches --tags --remotes \
            --graph --notes --source --abbrev-commit \
            --decorate=full --stat --show-signature \
            --first-parent
    p = pull
    pushd = push -u origin HEAD
    co = checkout
    b = branch
    undo = reset --soft HEAD~1
[pull]
    rebase = true
[rebase]
    autoStash = true
[push]
    default = simple
    followTags = true
[core]
    excludesfile = ~/.gitignore_global
[user]
    name = John Flinchbaugh
    email = john@hjsoft.com
[init]
	defaultBranch = main
